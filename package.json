{
  "name": "hugo-test-project",
  "version": "1.0.0",
  "description": "github pages with hugo test project",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "hugo --config hugo.json,docs/config.json",
    "server": "hugo server -b http://localhost:1313/ --bind 0.0.0.0 --gc --config hugo.json,docs/config.json",
    "preview": "hugo server -b http://localhost:1313/ --bind 0.0.0.0 -D -F --gc --config hugo.json,docs/config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iswai/hugo-test-project.git"
  },
